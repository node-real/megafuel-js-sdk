  "license": "GPL-3.0-or-later",
  "keywords": [
    "megafuel"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./dist/tsc/index.js",
  "types": "./dist/tsc/index.d.ts",
