    "esbuild": "^0.15.18",
    "eslint": "^8.29.0",
    "jest": "29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "ethers": "^6.13.2"
  }
